{
    "name": "nlf/specification",
    "description": "A boilerplate component for Specification pattern (Eric Evans)",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "Nlf\\Component\\Pattern\\Specification\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nlf\\Component\\Pattern\\Specification\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
